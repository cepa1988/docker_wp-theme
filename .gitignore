# Ignore WordPress core files (if you don't want to commit them)
/wp-admin/
/wp-includes/
wp-config.php
# Ignore all themes except the custom-theme
/wp-content/themes/*
!/wp-content/themes/custom-theme

# Ignore the node_modules folder inside the custom-theme
/wp-content/themes/custom-theme/node_modules/

/wp-content/plugins/akismet

# Ignore WordPress database data (persistent data for WordPress)
/wordpress_data/

# Ignore Docker-related files
.docker-compose.override.yml
docker-compose.override.yml


# Ignore OS-generated files
.DS_Store
Thumbs.db

# Ignore any log files
*.log
